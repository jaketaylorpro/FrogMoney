extends layout
block content
    h1= "welcome to " + title + ";the time is: " + time
    h3.chead active users
    div.cpanel
        if users == null || users.length ==0
            p no users
        else
            div.FMDataTable
                table
                    thead
                        tr
                            each key in Object.keys(users[0])
                                td=key
                    tbody
                        each user in users
                            tr
                                each key in Object.keys(user)
                                    td=user[key]
    h3.chead all expenses
    div.cpanel
        if users == null || users.length ==0
            p no expenses
        else
            div.FMDataTable
                table
                    thead
                        tr
                            each key in Object.keys(expenses[0])
                                td=key
                    tbody
                        each expense in expenses
                            tr
                                each key in Object.keys(expense)
                                    td=expense[key]